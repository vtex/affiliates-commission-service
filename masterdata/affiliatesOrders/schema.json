{
  "$schema": "http://json-schema.org/schema#",
  "title": "AffiliatesOrders",
  "type": "object",
  "properties": {
    "affiliateId": {
      "type": "string"
    },
    "status": {
      "type": "string"
    },
    "userEmail": {
      "type": "string"
    },
    "orderTotal": {
      "type": "number"
    },
    "orderTotalCommission": {
      "type": "number"
    },
    "orderDate": {
      "type": "string"
    },
    "orderItems": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "skuId": {
            "type": "string"
          },
          "skuName": {
            "type": "string"
          },
          "skuImageUrl": {
            "type": "string"
          },
          "price": {
            "type": "number"
          },
          "quantity": {
            "type": "number"
          },
          "commission": {
            "type": "number"
          }
        },
        "required": ["skuId", "skuName", "price", "quantity", "commission"]
      }
    }
  },
  "required": ["affiliateId", "userEmail", "orderItems"],
  "v-indexed": ["affiliateId", "userEmail", "status", "orderDate"],
  "v-cache": false
}
